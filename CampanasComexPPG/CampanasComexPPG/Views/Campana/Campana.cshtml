                                                        <tr ng-repeat="d in ListMecanicaTiendas">
                                                            <td class="d-none">{{d.IdCampania}}</td>
                                                            <td>{{d.ClaveCampania}}</td>
                                                            <td>{{d.Id_Tienda}}</td>
                                                            <td>{{d.Bill_To}}</td>
                                                            <td>{{d.Customer_Name}}</td>
                                                            <td>{{d.Region}}</td>
                                                            <td>{{d.Descripcion_Region}}</td>
                                                            <td>{{d.Descripcion_Zona}}</td>
                                                            <td>{{d.Segmento}}</td>
                                                            <td>{{d.Clave_Sobrepecio}}</td>
                                                        </tr>
                                                </table>

                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>

                        </form>
                    </div>

                    @*Pie Modal*@
                    <div class="modal-footer div-centrar">

                        <div id="divSave" ng-class="cssDivSaveModalButton">
                            <div class="form-group row">
                                <div class="col-md-6 div-centrar">
                                    <button id="cancelarModal" type="button" class="btn btn-primary" data-dismiss="modal" ng-click="">
                                        <i class="fa fa-window-close fa-lg" aria-hidden="true"></i> Cerrar
                                    </button>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <br />

        @*MODAL AGREGAR EDITAR USUARIO LDAP*@
        <div class="modal fade" id="myModalAddUsuarioLdap" tabindex="-1" role="document" data-keyboard="false" data-backdrop="static">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">

                    @*Encabezado Modal*@
                    <div class="modal-header">
                        <div class="navbar-header navbar-header-modal row">
                            <img src="~/Images/PPG_Layout.png" class="col-sm-2 img-rounded img-modal-ppg" alt="Cinque Terre">
                            <img src="~/Images/comex_layout_2.png" class="col-sm-4 img-rounded img-modal-comex" alt="Cinque Terre">
                            <h5 id="modalTitle" class="col-sm-6 modal-title">Editar Usuario</h5>
                        </div>

                        <button type="button" class="close d-none" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>

                    @*Cuerpo Modal*@
                    <div class="modal-body modal-body-max">
                        @*Formulario*@
                        <form class="form-horizontal">
                            <input type="hidden" id="id_indicador" />

                            @*PPGID*@
                            <div class="form-group input-group">
                                <label class="control-label col-sm-2" for="ppgid">PPGID:</label>
                                &nbsp;
                                <div class="input-group-append">
                                    <input type="text" class="form-control text-uppercase" id="ppgid" name="ppgid" placeholder="PPGID"
                                           ng-model="PPGIDLdap" required ng-readonly="ReadOnlyCampos">
                                </div>
                            </div>

                            @*Correo*@
                            <div class="form-group input-group">
                                <label class="control-label col-sm-2" for="ppgid">Correo:</label>
                                &nbsp;
                                <div class="input-group-append">
                                    <input type="text" class="form-control text-lowercase" id="correo" name="correo" placeholder="Correo"
                                           ng-model="CorreoLdap" required ng-readonly="ReadOnlyCampos">
                                </div>
                            </div>

                            @*Usuario*@
                            <div class="form-group input-group">
                                <label class="control-label col-sm-2" for="ppgid">Nombre:</label>
                                &nbsp;
                                <div class="input-group-append">
                                    <input type="text" class="form-control text-uppercase" id="nombre" name="nombre" placeholder="Nombre" style="width:350px;max-width:450px;"
                                           ng-model="NombreLdap" required ng-readonly="ReadOnlyCampos">
                                </div>
                                &nbsp;
                                <div class="input-group-append">
                                    <button type="button" class="btn btn-primary" ng-disabled="ValidateSearchUsuarioLdap(PPGIDLdap, CorreoLdap, NombreLdap)"
                                            ng-click="SearchUsuarioLdap(PPGIDLdap, CorreoLdap, NombreLdap)">
                                        <i class="fa fa-search fa-lg" aria-hidden="true"></i>Buscar
                                    </button>
                                </div>
                            </div>


                            @*Rol*@
                            @*<div class="form-group input-group">
                                <label class="control-label col-sm-2" for="rol">Rol:</label>
                                &nbsp;&nbsp;&nbsp;&nbsp;
                                <div class="input-group-append">
                                    <select class="form-control" id="negocioLider" ng-model="RolCronogramaAdd" ng-options="data as data.Rol for data in ListRolCronograma | orderBy:'ID' track by data.ID" required>
                                        <option value="">-- SELECCIONE --</option>
                                    </select>
                                </div>
                            </div>*@


                            @*TABLA USUARIOS*@
                            <div id="divTabla" class="col-md-auto table-responsive tabla-height">
                                <table class="table table-sm table-small-font table-bordered table-striped table-hover">
                                    <thead class="bg-blue">
                                        <tr>
                                            <th>Seleccionar</th>
                                            <th>PPGID</th>
                                            <th>Correo</th>
                                            <th>Usuario</th>
                                        </tr>
                                    </thead>
                                    <tbody class="noMostrarDiv" ng-class="mostrarDiv">
                                        <tr ng-repeat="d in ListUsuarioLdapTemp">
                                            <td style="text-align:center;vertical-align:middle;">
                                                <input type="radio" class="radio-usuario" name="seleccionUsuarioLdap" ng-model="d.Seleccionar" ng-click="SeleccionarUsuarioLdap(d)" />
                                            </td>
                                            <td>{{d.PPGID}}</td>
                                            <td>{{d.Email}}</td>
                                            <td>{{d.Nombre}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>


                        </form>
                    </div>

                    @*Pie Modal*@
                    <div class="modal-footer div-centrar">

                        <div id="divEdit">
                            <div class="form-group row">
                                <div class="col-md-6 div-centrar">
                                    <button ng-disabled="ValidateEditUsuarioLdap(SeleccionUsuarioLdap)" id="saveModal" type="button" class="btn btn-primary" data-dismiss="modal" ng-click="EditUsuarioLdap(UsuarioLdap, Cronograma, TipoUduarioLdap)">
                                        <i class="fa fa-save fa-lg" aria-hidden="true"></i> Editar
                                    </button>
                                </div>
                                <div class="col-md-6 div-centrar">
                                    <button id="cancelarModal" type="button" class="btn btn-secondary" data-dismiss="modal" ng-click="">
                                        <i class="fa fa-arrow-left fa-lg" aria-hidden="true"></i> Cancelar
                                    </button>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>

        <br />

    </div>
</body>