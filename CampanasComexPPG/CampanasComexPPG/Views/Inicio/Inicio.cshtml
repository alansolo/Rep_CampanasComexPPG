
@{
    ViewBag.Title = "Inicio";
    Layout = "~/Views/Shared/_LayoutCampana.cshtml";
}

<h2></h2>

<head>

    <link href="~/Content/Inicio/Inicio.css" rel="stylesheet" />
    <script src="~/Scripts/Inicio/Inicio.js"></script>
</head>

<body id="body">
    <div ng-app="MyApp" ng-controller="MyController">
        <input id="CorreoSesion" type="text" value="" class="d-none"/>

        <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
            <ol class="carousel-indicators">
                <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
                <li data-target="#carouselExampleIndicators" data-slide-to="5"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100 img" src="~/Images/comex_fondo_1a.png" alt="2 slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100 img" src="~/Images/comex-fondo-2a.png" alt="3 slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100 img" src="~/Images/comex-fondo-3a.png" alt="1 slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100 img" src="~/Images/Comex_2.png" alt="4 slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100 img" src="~/Images/Comex_1.png" alt="5 slide">
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true" title="Atras."></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true" title="Adelante."></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </div>
</body>